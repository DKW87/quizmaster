<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/10.0.2-internal"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.StudentSignInOutController">
    <Label layoutX="14.0" layoutY="14.0" text="In- en uitschrijven voor een cursus">
        <font>
            <Font size="18.0"/>
        </font>
    </Label>
    <Label layoutX="14.0" layoutY="69.0" text="Selecteer cursussen waar je je voor wilt inschrijven">
        <font>
            <Font size="16.0"/>
        </font>
    </Label>
    <ListView fx:id="signedOutCourseList" layoutX="11.0" layoutY="96.0" prefHeight="200.0" prefWidth="400.0"/>
    <Button layoutX="425.0" layoutY="152.0" mnemonicParsing="false" onAction="#doSignIn" text="-&gt;">
        <font>
            <Font size="18.0"/>
        </font>
    </Button>
    <Button layoutX="425.0" layoutY="208.0" mnemonicParsing="false" onAction="#doSignOut" text="&lt;-">
        <font>
            <Font size="18.0"/>
        </font>
    </Button>
    <Label layoutX="483.0" layoutY="69.0" text="Selecteer cursussen waar je je voor wilt uitschrijven">
        <font>
            <Font size="16.0"/>
        </font>
    </Label>
    <ListView fx:id="signedInCourseList" layoutX="483.0" layoutY="96.0" prefHeight="200.0" prefWidth="400.0"/>
    <Button layoutX="11.0" layoutY="309.0" mnemonicParsing="false" onAction="#doMenu" text="Menu">
        <font>
            <Font size="14.0"/>
        </font>
    </Button>
</AnchorPane>
